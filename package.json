  "scripts": {
    "start": "npm run copy-pdf-worker && vite",
    "dev": "npm run copy-pdf-worker && vite",
    "prebuild": "node scripts/generate-build-info.cjs",
    "build": "npm run copy-pdf-worker && tsc -b && vite build",
    "build:apk": "node build-apk.cjs",
    "build:aab": "node build-aab.cjs",
    "generate-build-info": "node scripts/generate-build-info.cjs",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "copy-pdf-worker": "node -e \"const fs = require('fs'); const path = require('path'); const src = path.join('node_modules', 'react-pdf', 'node_modules', 'pdfjs-dist', 'build', 'pdf.worker.min.mjs'); const dest = path.join('public', 'pdf-worker', 'pdf.worker.min.mjs'); fs.mkdirSync(path.dirname(dest), { recursive: true }); fs.copyFileSync(src, dest); console.log('âœ“ PDF worker copied from react-pdf version to public/pdf-worker/');\""
  },
